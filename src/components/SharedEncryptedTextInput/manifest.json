{
  "displayName": "Encrypted TextInput",
  "defaultWidth": 160,
  "defaultHeight": 40,
  "resizeY": true,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "text",
      "displayName": "Text",
      "type": "text",
      "role": "formValue"
    },
    {
      "name": "borderColor",
      "displayName": "Border Color",
      "type": "color",
      "default": "#00A898"
    },
    {
      "name": "borderWidth",
      "displayName": "Border Width",
      "type": "number",
      "default": 1
    },
    {
      "name": "borderRadius",
      "displayName": "Border Radius",
      "type": "number",
      "default": 5
    },
    {
      "name": "color",
      "displayName": "Color",
      "type": "color",
      "default": "#00A898"
    },
    {
      "name": "theirPublicKey",
      "displayName": "Their public key to be used for encryption",
      "type": "text"
    },
    {
      "name": "myPrivateKey",
      "displayName": "My private key to be used for encryption",
      "type": "text"
    },
    {
      "name": "myPublicKey",
      "displayName": "My public key to be used for encryption",
      "type": "text"
    }
  ]
}